<%- include('partials/header') %>

<h1>Home</h1>
<p><%= homeContent %></p>

<% posts.forEach(function(post){ %>
    <h1><%= post.title %></h1>
    <% let postTitleToURL = (post.title) %>
    <p><%= post.body.substring(0,100) %>... <a href="/posts/<%= postTitleToURL %>">Read More</a></p>
    
<% }); %>

<%- include('partials/footer') %>